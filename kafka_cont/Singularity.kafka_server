Bootstrap:docker
From:wurstmeister/kafka

%environment
	KAFKA_ADVERTISED_HOST_NAME=${HOSTNAME}
	KAFKA_ZOOKEEPER_CONNECT=${HOSTNAME}:2181
	KAFKA_ADVERTISED_PORT=9092
	KAFKA_CREATE_TOPICS="emb:1:1,spark_emb:1:1"	
	export KAFKA_ADVERTISED_HOST_NAME KAFKA_ZOOKEEPER_CONNECT KAFKA_ADVERTISED_PORT KAFKA_CREATE_TOPICS
